package rpg.item;

public class Item {
	private Rarity rarity;
	private double dropRate;

	public Rarity getRarity()
	{
		return rarity;
	}

	/**
	 * @return the dropRate
	 */
	public double getDropRate()
	{
		return dropRate;
	}

	/**
	 * @param dropRate the dropRate to set
	 */
	public void setDropRate( double dropRate )
	{
		this.dropRate = dropRate;
	}

}
